<%= form_for @exercise do |f| %>
<%= render "layouts/errors", :object => @exercise %>
  <%= f.label :description %>
  <%= f.text_field :description %><br>
  <%= f.label :activity %>
  <%= f.text_field :activity %><br>
  <%= f.label :calories %>
  <%= f.text_field :calories %><br>
  <%= f.label :date %>
  <%= f.date_field :date %><br>
  <%= f.submit %>
<% end %>
